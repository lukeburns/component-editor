{
  "name": "component-editor",
  "version": "0.2.2",
  "description": "a react component that exposes a graphical interface for building react components from a palette of react components",
  "main": "dist/index.js",
  "scripts": {
    "start": "budo example/index.js --live -- -g browserify-css -t [ babelify --presets [ @babel/preset-env @babel/preset-react ] ]",
    "json": "budo json-example/index.js --live -- -g browserify-css -t [ babelify --presets [ @babel/preset-env @babel/preset-react ] ]",
    "build": "babel src --out-dir dist --copy-files"
  },
  "author": "Luke Burns <lukemburns@gmail.com",
  "contributors": [
    "Ilona Brand <theilonabrand@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babelify": "^10.0.0",
    "browserify-css": "^0.15.0",
    "budo": "^11.6.2",
    "prop-types": "^15.6.2",
    "react-dom": "^16.6.3"
  },
  "dependencies": {
    "draft-convert": "^2.1.4",
    "draft-js": "^0.10.5",
    "react": "^16.6.3",
    "react-keyed-file-browser": "^1.4.3",
    "react-pack-unpack": "^0.1.0"
  }
}
